    <validationRules>
        <fullName>Category_shoe_size</fullName>
        <active>true</active>
        <description>Validates that the shoe size is right for the selected category.</description>
        <errorConditionFormula>OR(
				AND(    
								ISPICKVAL(Category__c, &apos;Men Shoes&apos;),
								OR(
												Size__c &lt; 39,
								    Size__c &gt; 50,
												ISNULL(Size__c)
								)
					),
				AND(    
								ISPICKVAL(Category__c, &apos;Women Shoes&apos;),
								OR(
								    Size__c &lt; 35,
								    Size__c &gt; 43,
												ISNULL(Size__c)
								)
					),
				AND(   
								ISPICKVAL(Category__c, &apos;Kid Shoes&apos;),
								OR(
								    Size__c &lt; 19,
								    Size__c &gt; 40,
												ISNULL(Size__c)
								)
				)
)</errorConditionFormula>
        <errorMessage>Invalid size value. Size help text contains the valid values.</errorMessage>
    </validationRules>
